# AI Provider 配置
VITE_AI_PROVIDER=dashscope  # 可选值: dashscope, deepseek, dobrain, moonshot, openai
VITE_AI_USE_PROXY=false
VITE_AI_PROXY_URL=https://api.deepseek.com

# DeepSeek 配置
VITE_DEEPSEEK_API_KEY=your_deepseek_api_key_here
VITE_DEEPSEEK_API_URL=https://api.deepseek.com/v1
VITE_DEEPSEEK_MODEL=deepseek-chat
VITE_DEEPSEEK_MAX_TOKENS=1000

# Moonshot 配置
VITE_MOONSHOT_API_KEY=your_moonshot_api_key_here
VITE_MOONSHOT_API_URL=https://api.moonshot.cn/v1
VITE_MOONSHOT_MODEL=moonshot-v1-8k
VITE_MOONSHOT_MAX_TOKENS=1000

# 豆包配置
VITE_DOBRAIN_API_KEY=your_dobrain_api_key_here
VITE_DOBRAIN_API_URL=https://ark.cn-beijing.volces.com/api/v3
VITE_DOBRAIN_MODEL=dobrain-v1
VITE_DOBRAIN_MAX_TOKENS=1000
VITE_DOBRAIN_TOP_P=0.7
VITE_DOBRAIN_PRESENCE_PENALTY=0
VITE_DOBRAIN_FREQUENCY_PENALTY=0

# OpenAI 配置
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_OPENAI_API_URL=https://api.openai.com/v1
VITE_OPENAI_MODEL=gpt-3.5-turbo
VITE_OPENAI_MAX_TOKENS=1000

# 阿里云 DashScope 配置
VITE_DASHSCOPE_API_KEY=your_dashscope_api_key_here
VITE_DASHSCOPE_API_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
VITE_DASHSCOPE_MODEL=deepseek-v3
VITE_DASHSCOPE_MAX_TOKENS=1000